// Forward declaration of isBadVersion API.
bool isBadVersion(int version);

int firstBadVersion(int n) {
    int low = 1, high = n;
    while (low < high)
    {
        int findbad = low + (high - low) / 2;
        if (isBadVersion(findbad))
        {
            high = findbad;
        }
        else
        {
            low = findbad + 1;
        }
    }
    return low;
}
